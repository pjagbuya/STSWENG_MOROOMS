<script id="edit-template" type="text/x-handlebars-template" >



  \{{#each data}}

    \{{#each seats}}
    <form>
      <tr>
        <td class="tc" >
          <span data-weekDay="\{{specificWeekDay}}" class="weekday-span">\{{weekDay}}</span><br>
          <select class="inputWeekDay">
          </select>
        </td>
        <td class="tc">
          <span data-timeID="\{{timeID}}" class="time-interval-span">\{{timeInterval}}</span><br>
          <select class="select-interval">
            <option data-timeID="1">7:00 A.M. - 7:30 A.M.</option>
            <option data-timeID="2">7:30 A.M. - 8:00 A.M.</option>
            <option data-timeID="3">8:00 A.M. - 8:30 A.M.</option>
            <option data-timeID="4">8:30 A.M. - 9:00 A.M.</option>
            <option data-timeID="5">9:00 A.M. - 9:30 A.M.</option>
            <option data-timeID="6">9:30 A.M. - 10:00 A.M.</option>
            <option data-timeID="7">10:00 A.M. - 10:30 A.M.</option>
            <option data-timeID="8">10:30 A.M. - 11:00 A.M.</option>
            <option data-timeID="9">11:00 A.M. - 11:30 A.M.</option>
            <option data-timeID="10">11:30 A.M. - 12:00 P.M.</option>
            <option data-timeID="11">12:00 P.M. - 12:30 P.M.</option>
            <option data-timeID="12">12:30 P.M. - 1:00 P.M.</option>
            <option data-timeID="13">1:00 P.M. - 1:30 P.M.</option>
            <option data-timeID="14">1:30 P.M. - 2:00 P.M.</option>
            <option data-timeID="15">2:00 P.M. - 2:30 P.M.</option>
            <option data-timeID="16">2:30 P.M. - 3:00 P.M.</option>
            <option data-timeID="17">3:00 P.M. - 3:30 P.M.</option>
            <option data-timeID="18">3:30 P.M. - 4:00 P.M.</option>
            <option data-timeID="19">4:00 P.M. - 4:30 P.M.</option>
            <option data-timeID="20">4:30 P.M. - 5:00 P.M.</option>

          </select>
        </td>
        <td class="tc">
          <span class="editable-seat-number-span">\{{seatNumber}}</span><br><button class="change-btn-seatnumber">Change</button></td>
        <td class="tc"><br><button class="btn btn-danger cancel-btn-seatnumber">Cancel Seat</button></td>
        <td class="tc"><br><button class="btn btn-success success-btn-seatnumber">Save Changes</button></td>

      </tr>
    </form>
    \{{/each}}
  \{{/each}}

</script>
