          </div> -->



          <!-- {{search-lab-modal}} -->
          <div class="d-flex flex-column filter-part-2" onclick="alert('Currently Unavailable')">
            <button class="custom-button-6 mx-5" type="button" name="button">Filter</button>
          </div>



        </div>




      </div>

      <div class="lab-choices-section">

          {{#each labs}}
          <div class="lab-choice-section">
            <img class="lab-choice-img" src="/pics/computerlab.jpg" alt="computer-lab">
            <div class="lab-choice-details">
              <h3>{{labName}}</h3>

              {{#if (isAvailable labStatus)}}
              <p class="status-text avail-text"><u>{{labStatus}}</u></p>
              {{else}}
              <p class="status-text full-text"><u>{{labStatus}}</u></p>
              {{/if}}
              <p>
                <span>Total: </span>
                <span><strong>{{labTotalSlots}}</strong></span><br>
                <span>Avail: </span>
                <span><strong>{{labAvailSlots}}</strong></span>
              </p>

              <div class="d-flex lab-choice-btn custom-default-font2">
                <div class="btn btn-success selectBtn-LT-Reserve" data-labname="{{labName}}">
                  Select
                </div>
              </div>


            </div>
          </div>
          {{/each}}

      </div>



      <div class="arrows-section">
        <button class="arrow-button">&larr;</button>  Page 1<button class="arrow-button right-arrow">&rarr;</button>
      </div>



    </div>
